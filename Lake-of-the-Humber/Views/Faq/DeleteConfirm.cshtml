@model Lake_of_the_Humber.Models.FaqDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Delete Faq #  @Model.FaqId</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Faq</h4>
    <hr />
    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">@Model.Question</h5>
            <h6 class="card-subtitle mb-2 text-muted">@Model.FaqDate</h6>
            <p class="card-text">@Model.Answer</p>
        </div>
    </div>

    @using (Html.BeginForm("Delete", "Faq", new { id = Model.FaqId })) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "List")
        </div>
    }
</div>
